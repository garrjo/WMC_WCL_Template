				activeImage="wa2.eq.on.active"
				tooltip="Toggle EQ processing"
				inactivealpha="128"
    />

			<button
				id="auto"
				action="EQ_AUTO"
				x="38" y="18"
				image="wa2.player.eq.auto"
				downImage="wa2.player.eq.auto.pressed"
				activeImage="wa2.player.eq.auto.active"
				tooltip="Toggle EQ automatic processing"
				inactivealpha="128"
