				x="264" y="3"
				image="wa2.player.eq.button.quit"
				downImage="wa2.player.eq.button.quit.pressed"
				tooltip="Close"
				inactiveAlpha="128"
    />

			<button
				id="winshade" tooltip="Toggle Windowshade mode"
				action="SWITCH" param="shade"
				x="254" y="3"
