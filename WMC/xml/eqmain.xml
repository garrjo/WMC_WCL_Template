	<layout id="shade" background="wa2.player.eq.shade">

		<layer
			id="shadetoeq"
			dblClickAction="SWITCH;normal"
			x="0" y="0"
			image="wa2.player.eq.shade"
			inactiveAlpha="0"
    />

		<button
			id="winshade"
			action="SWITCH" param="normal"
			x="254" y="3"
			image="wa2.player.title.shade"
			downImage="wa2.player.title.shade.pressed"
			inactiveAlpha="128"
			tooltip="Toggle Windowshade mode"
    />

		<button
			id="Close"
			action="close"
			x="264" y="3"
			image="wa2.player.eq.shade.button.quit"
			downImage="wa2.player.eq.shade.button.quit.pressed"
			tooltip="Close"
			inactiveAlpha="128"
    />
		<images source="volume" images="wa2.player.vol.main" x="80" y="5" w="68" h="13" imagesspacing="15" inactiveAlpha="128"/>
		<slider id="Volume"
						action="VOLUME"
						x="80" y="3" w="68" h="13"
						thumb="wa2.player.vol.slider.normal" downThumb="wa2.player.vol.slider.pressed" inactiveAlpha="128"/>

		<images source="balance"
						 x="170" y="5" w="36" h="13"
						images="wa2.player.bal.main"
						imagesspacing="15"/>
		<slider id="Pan"
						action="PAN"
						x="173" y="3" w="38" h="13"
						thumb="wa2.player.bal.slider.normal" downThumb="wa2.player.bal.slider.pressed" inactiveAlpha="128"/>
