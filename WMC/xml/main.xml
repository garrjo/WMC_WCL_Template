<container id="Main" name="WA2 Skin Main window">
  <layout id="normal" background="wa2.player.base">
    <layer id="mainview" backround="wa2.player.background" x="0" y="0" image="wa2.player.background" w="275" h="116"/>
    <layer
			id="PosBarBack"
			x="16" y="72"
			w="248"
			image="wa2.player.posbar.bg"
			rectrgn="0"
			move="0"
			gammagroup="Button"
    />
    <ProgressGrid
		x="17"
		y="75"
		w="248"
		h="10"
		relatw="0"
		orientation="left"
		gammagroup="Slider"
		/>

    <Button
			id="Previous"
			action="Prev"
			x="16" y="88"
			image="wa2.player.prev"
			downImage="wa2.player.prev.down"
			tooltip="Previous"
			inactivealpha="128"
			gammagroup="Button"
    />

    <Button
			id="Play"
			action="Play"
			x="39" y="88"
			image="wa2.player.play"
			downImage="wa2.player.play.down"
			tooltip="Play"
			 inactivealpha="128"
			gammagroup="Button"
    />

    <Button
			id="Pause"
			action="Pause"
			x="62" y="88"
			image="wa2.player.pause"
			downImage="wa2.player.pause.down"
			tooltip="Pause"
			 inactivealpha="128"
			gammagroup="Button"
    />

    <Button
			id="Stop"
			action="Stop"
			x="84" y="88"
			image="wa2.player.stop"
			downImage="wa2.player.stop.down"
			tooltip="Stop"
			 inactivealpha="128"
			gammagroup="Button"
    />

    <Button
			id="Next"
			action="Next"
			x="108" y="88"
			image="wa2.player.next"
			downImage="wa2.player.next.down"
			tooltip="Next"
			 inactivealpha="128"
			gammagroup="Button"
    />

    <Button
			id="Eject"
			action="EJECT"
			x="130" y="88"
			image="wa2.player.eject"
			downImage="wa2.player.eject.down"
			tooltip="Eject"
			 inactivealpha="128"
			gammagroup="Button"
    />
    <text
		 id="infoline"
		 display="songinfo" default=""
		 x="30" y="200" h="20" w="50"
			showlen="1"
			antialias="true"
			timeroffstyle="1"
			offsetx="1"
			offsety="1"
			shadowcolor="0,0,0"
			shadowx="0"
			shadowy="0"
			alpha="255"
			color="200,0,0"
			font="wa2.player.SmallFixed" fontsize="8"
			inactivealpha="128"
			gammagroup="Text"
     />
    <text
			id="songticker" ticker="1"
			display="songname" default="" showlen="2"
			x="111" y="22"
			w="154" h="10"
			antialias="false"
			timeroffstyle="0"
			offsetx="0"
			offsety="0"
			shadowcolor="0,44,0"
			shadowx="0"
			shadowy="1"
			alpha="150"
			color="130,130,130"
			font="wa2.player.SmallFixed"
			inactivealpha="128"
			gammagroup="Text"
    />
    <text
			id="timer"
			display="time"
			x="40" y="15"
			w="100" h="27"
			default="" showlen="0"
			antialias="1"
			timeroffstyle="0"
			shadowcolor="0,0,0"
			shadowx="0"
			shadowy="0"
			timecolonwidth="7" fontsize="30"
			alpha="200" vspace="0" hspace="5"
			color="0,50,50"
			font="wa2.player.ttf.BIGNUM"
			inactivealpha="128"
			gammagroup="Text"
    />
    <vis
			id="vis"
			x="25" y="38"
			w="72" h="32"
			alpha="220"
			default="spectrum"
			colorband1="86,0,45"
			colorband2="239,0,177"
			colorband3="239,0,177"
			colorband4="196,0,110"
			colorband5="196,0,110"
			colorband6="160,0,90"
			colorband7="160,0,90"
			colorband8="127,0,71"
			colorband9="127,0,71"
			colorband10="97,0,54"
			colorband11="97,0,54"
			colorband12="75,0,41"
			colorband13="75,0,41"
			colorband14="86,0,45"
			colorband15="86,0,45"
			colorband16="86,0,45"
			colorosc1="210,210,210"
			colorosc2="174,174,174"
			colorosc3="138,138,138"
			colorosc4="102,102,102"
			colorosc5="66,66,66"
			colorbandpeak="229,0,131"
			inactivealpha="128"
			gammagroup="Vis"
				/>
    <text
			id="bitrateText"
			ticker="1"
			display=""
			x="109" y="41"
			w="20" h="11"
			font="wa2.player.SmallFixed"
			fontsize="8"
			align="left"
			alpha="120"
			color="81,83,86"
			inactivealpha="128"
     />
    <text
			id="frequency"
			ticker="1"
			display=""
			color="81,83,86"
			default=""
			x="154" y="41"
			w="20" h="11"
			font="wa2.player.SmallFixed"
			fontsize="7"
			align="left"
			alpha="120"
			inactivealpha="128"
    />
    <text
			id="kbps"
			ticker="1"
			default="kbps"
			x="127" y="40"
			w="30" h="11"
			font="wa2.player.SmallFixed"
			antialias="true"
			offsetx="1"
			offsety="1"
			shadowcolor="0,0,0"
			shadowx="0"
			shadowy="0"
			alpha="200"
			color="81,83,86"
			fontsize="7"
			 inactivealpha="128"
     />
    <text
			id="Khz"
			ticker="1"
			default="khz"
			x="165" y="40"
			w="40" h="11"
			fontsize="7"
			font="wa2.player.SmallFixed"
			antialias="true"
			offsetx="1"
			offsety="1"
			shadowcolor="0,0,0"
			shadowx="0"
			shadowy="0"
			timecolonwidth="0"
			alpha="200"
			color="81,83,86"
			inactivealpha="128"
			gammagroup="Text"
     />
    <toggleButton
			id="eq"
			action="TOGGLE" param="EQ"
			x="219" y="58"
			w="23" h="12"
			image="wa2.player.eq.off"
			downImage="wa2.player.eq.down"
			activeImage="wa2.player.eq.on"
			tooltip="Toggle Equalizer"
			 inactivealpha="128"
			gammagroup="Button"
    />

    <toggleButton
			id="pl"
			action="TOGGLE" param="PL"
			x="242" y="58"
			w="23" h="12"
			image="wa2.player.playlist.off"
			downImage="wa2.player.playlist.down"
			activeImage="wa2.player.playlist.on"
			tooltip="Toggle Playlist Editor"
			rectrgn="1"
			 inactivealpha="128"
			gammagroup="Button"
    />

    <toggleButton
			id="Repeat"
			cfgattrib="{45F3F7C1-A6F3-4EE6-A15E-125E92FC3F8D};Repeat"
			x="210" y="89"
			action="TOGGLE_REPEAT"
			image="wa2.player.rpt.off"
			downImage="wa2.player.rpt.down"
			activeImage="wa2.player.rpt.on"
			tooltip="Repeat"
			 inactivealpha="128"
			gammagroup="Button"
    />

    <toggleButton
			id="Shuffle"
			cfgattrib="{45F3F7C1-A6F3-4EE6-A15E-125E92FC3F8D};Shuffle"
			action="TOGGLE_SHUFFLE"
			x="164" y="89"
			image="wa2.player.sfl.off"
			downImage="wa2.player.sfl.down"
			activeImage="wa2.player.sfl.on"
			tooltip="Shuffle"
			 inactivealpha="128"
			gammagroup="Button"
    />

    <layer
			id="maintoshade"
			dblClickAction="SWITCH;shade"
			x="0" y="0"
			image="wa2.player.title.enabled"
			inactiveAlpha="128"
    />
    <images source="volume" images="wa2.player.vol.main" x="107" y="57" w="68" h="13" imagesspacing="15" inactivealpha="128"/>
    <slider id="Volume"
						action="VOLUME"
						x="109" y="57" w="68" h="13"
						thumb="wa2.player.vol.slider.normal" downThumb="wa2.player.vol.slider.pressed" inactivealpha="128"/>

    <images source="balance" x="179" y="57" w="36" h="13" images="wa2.player.bal.main" imagesspacing="15" inactiveAlpha="128"/>
    <slider id="Pan"
						action="PAN"
						x="181" y="57" w="38" h="13"
						thumb="wa2.player.bal.slider.normal" downThumb="wa2.player.bal.slider.pressed" inactivealpha="128"/>
    <Button
			id="Close"
			action="close"
			x="264" y="3"
			image="wa2.player.title.quit"
			downImage="wa2.player.title.quit.pressed"
			tooltip="Close"
			inactiveAlpha="128"
			gammagroup="Button"
    />

    <Button
			id="winshade" tooltip="Toggle Windowshade mode"
			action="SWITCH" param="shade"
			x="255" y="3"
			image="wa2.player.title.shade"
			downImage="wa2.player.title.shade.pressed"
			inactiveAlpha="128"
			gammagroup="Button"
    />

    <Button
			id="Minimize"
			action="MINIMIZE"
			x="245" y="3"
			image="wa2.player.title.min"
			downImage="wa2.player.title.min.pressed"
			tooltip="Minimize Winamp"
			inactiveAlpha="128"
			gammagroup="Button"
    />

    <Button
			id="sysButton"
			action="SYSMENU"
			x="6" y="3"
			image="wa2.player.title.menu"
			downImage="wa2.player.title.menu.pressed"
			inactiveAlpha="128"
			gammagroup="Button"
    />

    <layer
			id="Mono"
			x="212" y="41"
			image="wa2.player.mono.off"
			inactiveAlpha="128"
			gammagroup="Text"
    />
    <layer
			id="Stereo"
			x="239" y="41"
			image="wa2.player.stereo.off"
			inactiveAlpha="128"
			gammagroup="Text"
    />
    <layer
			id="monspeaker"
			x="212" y="41"
			image="wa2.player.mono.on"
			gammagroup="Text"
    />
    <layer
			id="sterspeaker"
			x="239" y="41"
			image="wa2.player.stereo.on"
			gammagroup="Text"
    />
    <status
			id="status"
			x="25" y="29"
			w="9" h="9"
			stopBitmap="player.status.stop"
			playBitmap="player.status.play"
			pauseBitmap="player.status.pause"
			gammagroup="Status"
    />
    <!--CLUSTER BAR-->
    <Button
			id="optiontog"
			action="TOGGLE" param="guid:{ED3CD0D9-5FE5-4C34-AFE9-52E714D4FAA4}"
			x="12" y="20"
			image="wa2.player.oiadv.o"
			downImage="wa2.player.oiadv.o.pressed"
			gammagroup="Button"
    />
    <toggleButton
			id="toggleAllWaysOnTop"
			x="12" y="30"
			image="wa2.player.oiadv.a"
			downImage="wa2.player.oiadv.a.pressed"
			activeImage="wa2.player.oiadv.a.pressed"
			tooltip="Always On Top"
			gammagroup="Button"
    />
    <toggleButton
			id="fileinfo"
			x="12" y="40"
			image="wa2.player.oiadv.i"
			downImage="wa2.player.oiadv.i.pressed"
			tooltip="File Info"
			gammagroup="Button"
    />
    <toggleButton
			id="toggledoublesize"
			x="12" y="50"
			image="wa2.player.oiadv.d"
			downImage="wa2.player.oiadv.d.pressed"
			activeImage="wa2.player.oiadv.d.pressed"
			tooltip="Double Size"
			gammagroup="Button"
    />
    <toggleButton
			id="vistog"
			x="12" y="60"
			image="wa2.player.oiadv.v"
			downImage="wa2.player.oiadv.v.pressed"
			gammagroup="Button"
    />
    <toggleButton
			id="Thinger"
			action="TOGGLE" param="thinger"
			x="12" y="70"
			h="14" w="14"
			tooltip="Thinger"
			gammagroup="Button"
    />
    <slider
			id="seeker"
			action="seek"
			x="19" y="72"
			h="10" w="250"
			thumb="wa2.player.posbar.up"
			downthumb="wa2.player.posbar.down"
			gammagroup="Slider"
    />
    <layer id="glass" background="wa2.player.glass" x="0" y="0" image="wa2.player.glass" w="275" h="116" gammagroup="Text" ghost="-1" />
  </layout>
  <layout id="shade" background="wa2.player.shade.enabled" gammagroup="Background2" inactive="wa2.player.shade.disabled">

    <layer
			id="blah"
			dblClickAction="SWITCH;normal"
			x="0" y="0"
			image="wa2.player.shade.enabled"
			inactiveAlpha="0"
    />

    <text
			id="timer"
			display="time"
			x="130" y="2"
			w="26" h="10"
			font="wa2.player.SmallFixed" fontsize="8"
    />

    <Button
			id="Previous"
			action="PREV"
			x="166" y="3"
			image="player.winshade.Button.previous"
			downImage="player.winshade.Button.previous"
			tooltip="Previous"
			inactiveAlpha="128"
			gammagroup="Button"
    />
    <Button
			id="Play"
			action="PLAY"
			x="176" y="3"
			image="player.winshade.Button.play"
			downImage="player.winshade.Button.play"
			tooltip="Play"
			inactiveAlpha="128"
			gammagroup="Button"
    />
    <Button
			id="Pause"
			action="PAUSE"
			x="186" y="3"
			image="player.winshade.Button.pause"
			downImage="player.winshade.Button.pause"
			tooltip="Pause"
			inactiveAlpha="128"
			gammagroup="Button"
    />
    <Button
			id="Stop"
			action="STOP"
			x="196" y="3"
			image="player.winshade.Button.stop"
			downImage="player.winshade.Button.stop"
			tooltip="Stop"
			inactiveAlpha="128"
			gammagroup="Button"
    />
    <Button
			id="Next"
			action="NEXT"
			x="206" y="3"
			image="player.winshade.Button.next"
			downImage="player.winshade.Button.next"
			tooltip="Next"
			inactiveAlpha="128"
			gammagroup="Button"
    />

    <Button
			id="Eject"
			action="EJECT"
			x="216" y="3"
			image="player.winshade.Button.open"
			downImage="player.winshade.Button.open"
			tooltip="Eject"
			gammagroup="Button"
    />

    <vis
			id="vis_Shade"
			x="79" y="5"
			w="38" h="5"
			gammagroup="Vis"

   />

    <Button
			id="winshade" tooltip="Toggle Windowshade mode"
			action="SWITCH" param="normal"
			x="254" y="3"
			image="wa2.player.title.shade"
			downImage="wa2.player.title.shade.pressed"
			inactiveAlpha="128"
			gammagroup="Button"
    />

    <Button
			id="Close"
			action="close"
			x="264" y="3"
			image="wa2.player.title.quit"
			downImage="wa2.player.title.quit.pressed"
			tooltip="Close"
			inactiveAlpha="128"
			gammagroup="Button"
    />

    <Button
			id="Minimize"
			action="MINIMIZE"
			x="243" y="3"
			image="wa2.player.title.min"
			downImage="wa2.player.title.min.pressed"
			tooltip="Minimize Winamp"
			gammagroup="Button"
    />

    <Button
			id="sysButton"
			action="SYSMENU"
			x="6" y="3"
			image="wa2.player.shade.menu"
			downImage="wa2.player.shade.menu.pressed"
			gammagroup="Button"
    />

    <slider
			id="seeker"
			action="seek"
			x="227" y="4"
			h="7" w="15"
			thumb="player.winshade.seek"
			downthumb="player.winshade.seek"
			gammagroup="Button"
    />
  </layout>

</container>