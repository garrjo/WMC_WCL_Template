<!-- Gamma sets -->
<gammaset id="Eroded">
	<gammagroup id="Background" value="0,0,0" gray="1"/>
	<gammagroup id="Background2" value="0,0,0" gray="1"/>
	<gammagroup id="Slider" value="0,0,0" gray="1"/>
</gammaset>

<gammaset id="Faust">
	<gammagroup id="Background" value="4000,-2000,-4000" gray="1"/>
	<gammagroup id="Background2" value="4000,0,-4000" gray="1"/>
	<gammagroup id="Slider" value="4000,4000,4000" gray="1"/>
</gammaset>

<gammaset id="FusionBox">
	<gammagroup id="Background" value="-2000,0,2000" gray="0"/>
	<gammagroup id="Background2" value="-2000,0,2000" gray="0"/>
	<gammagroup id="Slider" value="-2000,0,2000" gray="0"/>	
	<gammagroup id="Text" value="-2000,0,2000" gray="0"/>		
</gammaset>

<gammaset id="Original" default="true">
	<gammagroup id="Background" value="0,0,0"/>
	<gammagroup id="Slider" value="0,0,0"/>
</gammaset>

<gammaset id="JadeTube">
	<gammagroup id="Background" value="-1000,50,-1000" gray="0"/>
	<gammagroup id="Background2" value="-1000,500,-500"  gray="0"/>
	<gammagroup id="Slider" value="-500,50,-1000" gray="0"/>
</gammaset>

<gammaset id="WhiteOut">
	<gammagroup id="Background" value="255,255,255" gray="0"/>
	<gammagroup id="Background2" value="255,255,255" gray="0"/>
	<gammagroup id="Slider" value="255,255,255" gray="0"/>
</gammaset>